          <!--<add NAME="HON0001" USER="4501031001" PASSWD="password7011" CLIENT="701" LANG="J" ASHOST="133.253.62.189" SYSNR="00" POOL_SIZE="5" MAX_POOL_SIZE="10"/> -->
