        </destinations>
      </DestinationConfiguration>
    </ClientSettings>

    <ServerSettings>
      <ServerConfiguration>
        <servers>
          <add NAME="NCO_SERVER" GWHOST="MY_GWHOST" GWSERV="MY_SAPGW" PROGRAM_ID="nRfcTestServer" REPOSITORY_DESTINATION="NCO_TESTS" REG_COUNT="1"/>
        </servers>
